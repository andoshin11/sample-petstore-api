{
  "name": "@andoshin11/sample-petstore-api",
  "version": "0.0.1",
  "main": "index.js",
  "author": "andoshin11 <shinglish11@gmail.com>",
  "typings": "types/index.d.ts",
  "license": "MIT",
  "scripts": {
    "build": "swagpack build ./src/index.yml -o ./dist/swagger.yml",
    "typegen": "yarn run build && openapi-ts-gen generate ./dist/swagger.yml --namespace PetStore --dist types",
    "lint": "yarn run build && node ./validate.js",
    "prepare": "yarn run build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run build"
    }
  },
  "pubishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "js-yaml": "^3.13.1",
    "oas-validator": "^3.3.1",
    "openapi-ts-gen": "^0.0.19",
    "swagpack": "^0.0.5"
  },
  "dependencies": {}
}
